{
  "presets": ["@babel/preset-env", "@babel/preset-typescript"],
  "plugins": ["add-module-exports"],
  "env": {
    "test": {
      "plugins": ["explicit-exports-references"]
    },
    "development": {
      "sourceMaps": "inline",
      "retainLines": true
    },
    "production": {
      "ignore": ["**/node_modules/", "**/*.spec.js"]
    }
  }
}
